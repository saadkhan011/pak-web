  .testimonials .card .content h2 {
    font-size: 18px;
    color: #fff;
  }

  .testimonials .card .content h2 span {
    color: #03a9f4;
    font-size: 14px;
    transition: 0.5s;
  }

  .testimonials .card:hover .content h2 span {
    color: #fff;
  }
</style>

